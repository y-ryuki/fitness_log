{
    "require": {
        "twbs/bootstrap": "5.0.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2"

    },
    "scripts":{
        "phpcs": "./vendor/bin/phpcs --standard=phpcs.xml",
